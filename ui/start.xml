            <CONTAINER layout="100,100" src="msnweather/msnweather.xml" align="relative" img="#110000ff"/>
            <LABEL layout="620,100" textAlign="bottom" text="https://developer.yahoo.com/weather" />
            <CONTAINER layout="100,100" src="yahooweather/yahooweather.xml" align="relative" img="#110000ff"/>
        </WINDOW>
    </UILAYOUT>

</MOML>